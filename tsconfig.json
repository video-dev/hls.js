{
  "compilerOptions": {
    "sourceMap": true,
    "allowJs": true,
    "module": "es6",
    "target": "ES5",
    "strict": false,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2015",
    ],
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist/**/*",
    "**/*.spec.*"
  ]
}
