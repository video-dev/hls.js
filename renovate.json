{
  "extends": ["config:base"],
  "labels": ["dependencies", "skip-change-log"],
  "commitMessagePrefix": "[skip netlify]",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "prCreation": "not-pending",
  "stabilityDays": 7,
  "vulnerabilityAlerts": {
    "addLabels": ["security"]
  },
  "major": {
    "addLabels": ["semver-major"]
  },
  "packageRules": [
    {
      "matchPackagePatterns": ["*"],
      "rangeStrategy": "bump"
    },
    {
      "matchDepTypes": ["devDependencies"],
      "rangeStrategy": "pin"
    },
    {
      "matchDepTypes": ["peerDependencies"],
      "rangeStrategy": "widen"
    }
  ]
}
